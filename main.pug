doctype html
html
  head
    title= title
    link(
      rel='icon'
      href='/assets/header.png'
      type='image/png'
      )
    link(
      rel='stylesheet'
      href='/assets/' + stylesheet
      type='text/css'
    )

  block top-links

  body(
    class= body_class
    )
    .body-contents
      block header
        header
          include ../tisko-header/main
      .main-contents#main-contents
        | !{reactHTML}

      .c-footer#c-footer
        include ../../modules/footer/main

  script.
    window.__PRELOADED_STATE__= !{JSON.stringify(preloadedState)};
  script(
    src='/assets/react-with-addons.js'
  )
  script(
    src='/assets/react-dom.js'
  )

  script(
    src='/assets/' + javascript
  )
